{
	"cli": {
		"version": ">= 15.0.12"
	},
	"build": {
		"production": {
			"channel": "production"
		},
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"ios": { "simulator": true },
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview",
			"android": {
				"buildType": "apk"
			},
			"ios": { "simulator": true }
		},
		"preview2": {
			"android": {
				"gradleCommand": ":app:assembleRelease"
			}
		},
		"preview3": {
			"developmentClient": true
		}
	},
	"submit": {
		"production": {}
	}
}
